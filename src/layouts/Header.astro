---
import Icon from '~/components/common/icon.astro';
---

<header class='header'>
  <div class='header_inner'>
    <h1 class='header_logo'>
      <Icon name='code' />
      <span class='header_logo-text'>
        <span class='logo-title'>Frontend dev kit</span>
        <span>Astro + scss + ts</span>
      </span>
    </h1>
    <button class='theme' type='button'>
      <Icon name='moon' type='regular' class='theme_icon' />
    </button>
  </div>
</header>
